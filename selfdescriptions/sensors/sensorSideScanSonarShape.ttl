	sh:property [ sh:path ssn-system:SystemProperty ;
				  sh:name "Dynamic Range" ;
				  sh:description "??... Dynamic Range in Anzahl Bits"@de ;
				  sh:description "??... dynamic range in number of bits"@en ;
