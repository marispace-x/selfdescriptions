gax-validation:sensorMultibeamEchosounderShape
	a sh:NodeShape ;
	sh:targetClass gax-trust-framework:DataResource ;
	sh:property [ sh:path ssn-system:SystemProperty ;
				  sh:name "acoustic center frequency" ;
				  sh:description "Akustische Frequenz die vom Sonar gesendet wird in [Hz]."@de ;
				  sh:description "Acoustic center frequency emitted by the sonar in [Hz]."@en ;
				  skos:example "400000" ;
				  sh:order 1 ;
				  sh:minCount 0 ;
				  sh:maxCount 2 ;
				  sh:datatype xsd:integer ] ;
	sh:property [ sh:path ssn-system:SystemProperty ;
				  sh:name "pulse length" ;
				  sh:description "Sonar Pulslänge die vom Sonar gesendet wird in [ms]."@de ;
				  sh:description "Sonar pulse length emitted by the sonar in [ms]."@en ;
				  skos:example "5" ;
				  sh:order 2 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:integer ] ;
	sh:property [ sh:path ssn-system:OperatingPowerRange ;
				  sh:name "output power" ;
				  sh:description "Energie die vom Sonar abgegeben wird in [J]."@de ;
				  sh:description "Energy being emitted by sensor in [J]."@en ;
				  skos:example "4" ;
				  sh:order 3 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:integer ] ;
	sh:property [ sh:path ssn-system:SystemProperty ;
				  sh:name "beam opening angle along track" ;
				  sh:description "Strahlöffnungswinkel in along track Richtung (auch bezeichnet als Along Track Strahlbreite) in [°]."@de ;
				  sh:description "Beam opening angle in along track direction (also called along track beam width) in [°]."@en ;
				  skos:example "1.0" ;
				  sh:order 4 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:decimal ] ;
	sh:property [ sh:path ssn-system:SystemProperty ;
				  sh:name "beam opening angle across track" ;
				  sh:description "Strahlöffnungswinkel in across track Richtung (auch bezeichnet als Across Track Strahlbreite) in [°]."@de ;
				  sh:description "Beam opening angle in across track direction (also called across track beam width) in [°]."@en ;
				  skos:example "1.0" ;
				  sh:order 5 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:decimal ] ;
	sh:property [ sh:path ssn-system:SystemProperty ;
				  sh:name "swath angle" ;
				  sh:description "Öffnungswinkel des Swath der von dem Sonar gesendet wird in [°]."@de ;
				  sh:description "Opening angle of the swath that is emitted by the sonar in [°]"@en ;
				  skos:example "50.0" ;
				  sh:order 6 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:decimal ] ;
	sh:property [ sh:path ssn-system:SystemProperty ;
				  sh:name "number of beams" ;
				  sh:description "Die Anzahl der Strahlen, die vom Sonar empfangen wird."@de ;
				  sh:description "The number of beams that are received by the sonar."@en ;
				  skos:example "512" ;
				  sh:order 7 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:integer ] ;
	sh:property [ sh:path ssn-system:SystemProperty ;
				  sh:name "sounding pattern" ;
				  sh:description "Das Abtastmuster der Strahlen."@de ;
				  sh:description "The sounding pattern of the beams."@en ;
				  skos:example "equidistant" ;
				  sh:order 8 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;

	sh:property [ sh:path ssn-system:OperatingRange ;
				  sh:name "operating temperature" ;
				  sh:description "Anwendungstemperatur in [°C]."@de ;
				  sh:description "Operating temperature in [°C]."@en ;
				  skos:example "0" ;
				  sh:order 9 ;
				  sh:minCount 0 ;
				  sh:maxCount 2 ;
				  sh:datatype xsd:integer
				  ] ;
	sh:property [ sh:path ssn-system:OperatingRange ;
				  sh:name "operating depth" ;
				  sh:description "Anwendungstiefe in [m]."@de ;
				  sh:description "operating depth in [m]."@en ;
				  skos:example "2000" ;
				  sh:order 10 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:integer
				  ] ;

	sh:property [ sh:path dcterms:temporal ;
				  sh:name "temporal coverage" ;
				  sh:description "Zeitliche Periode in der die Datenresource aufgenommen worden ist."@de ;
				  sh:description "Temporal coverage of when the data resource was acquired."@en ;
				  skos:example "'(a structure object of type CPU)'" ;
				  sh:order 11 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:node dcterms:PeriodOfTime ] ;
	sh:property [ sh:path dcterms:spatial ;
				  sh:name "spatial coverage" ;
				  sh:description "Räumliche Abdeckung wo die Datenresource aufgenommen worden ist."@de ;
				  sh:description "Spatial coverage of where the data resource was acquired."@en ;
				  skos:example "'(a structure object of type CPU)'" ;
				  sh:order 12 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:node dcterms:Location ] ;
.


dcterms:PeriodOfTime
	a sh:NodeShape ;
	sh:targetClass dcterms:PeriodOfTime ;
	sh:property [ sh:path dcat:startDate ;
				  sh:name "start date" ;
				  sh:description "Startdatum der Datenaufnahme (Datum Uhrzeit im ISO 8601-Format)."@de ;
				  sh:description "Start date of the data acquisition (date time in ISO 8601 format)."@en ;
				  skos:example "'2022-10-26T21:32:52'" ;
				  sh:order 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:dateTime ] ;
	sh:property [ sh:path dcat:endDate ;
				  sh:name "end date" ;
				  sh:description "Enddatum der Datenaufnahme (Datum Uhrzeit im ISO 8601-Format)."@de ;
				  sh:description "End date of the data acquisition (date time in ISO 8601 format)."@en ;
				  skos:example "'2022-10-26T21:32:52'" ;
				  sh:order 2 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:dateTime ] ;
.

dcterms:Location
	a sh:NodeShape ;
	sh:targetClass dcterms:Location ;
	sh:property [ sh:path locn:geometry ;
				  sh:name "spatial coverage" ;
				  sh:description "Geometrische Information über die räumliche Abdeckung (z.B., as WKT, as KML, as GeoJSON)."@de ;
				  sh:description "Geometry information of spatial coverage (e.g., as WKT, as KML, as GeoJSON)."@en ;
				  skos:example "'e.g., as WKT, as KML, as GeoJSON - https://opengeospatial.github.io/ogc-geosparql/geosparql11/index.html#Geometry'" ;
				  sh:order 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
.