## General Sensor Information ##
gax-validation:sensorGeneralInformationShape
	a sh:NodeShape ;
	sh:targetClass gax-trust-framework:SensorGeneralInformation ;
	### general attributes ###
	sh:property [ sh:path device:hasManufacturer ;
				  sh:name "manufacturer" ;
				  sh:description "Hersteller (Firma), die den Sensor produziert hat."@de ;
				  sh:description "Manufacturer of the sensor."@en ;
				  skos:example "Teledyne Reson" ;
				  sh:order 1 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path device:hasModelID ;
				  sh:name "model id" ;
				  sh:description "Model-ID des Sensors."@de ;
				  sh:description "Model ID of sensor."@en ;
				  skos:example "SeaBat T20-P" ;
				  sh:order 2 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path device:hasModelIDSynonym ;
				  sh:name "model name" ;
				  sh:description "Modelname des Sensors."@de ;
				  sh:description "Model name of sensor."@en ;
				  skos:example "SeaBat" ;
				  sh:order 3 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path device:hasSerialNumber;
				  sh:name "serial number" ;
				  sh:description "Die Seriennummer, die eindeutig für diesen Sensor ist."@de ;
				  sh:description "The unique serial number of this sensor."@en ;
				  skos:example "1234-5678-910" ;
				  sh:order 4 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path device:hasWebSite;
				  sh:name "website" ;
				  sh:description "Link zu einer Webseite mit Sensorinformation."@de ;
				  sh:description "Link to website with sensor information."@en ;
				  skos:example "..." ;
				  sh:order 5 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	### contact attributes ###
	sh:property [ sh:path device:hasOwner;
				  sh:name "sensor owner" ;
				  sh:description "Besitzer des Sensors."@de ;
				  sh:description "Owner of sensor."@en ;
				  skos:example "company A" ;
				  sh:order 6 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:node vcard:Individual ] ;
	sh:property [ sh:path device:hasProvider;
				  sh:name "sensor provider" ;
				  sh:description "Anbieter des Sensor, der die Bereitstellung übernimmt."@de ;
				  sh:description "Provider of sensor."@en ;
				  skos:example "company B" ;
				  sh:order 7 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:node vcard:Individual ] ;
	sh:property [ sh:path device:hasPointOfContact;
				  sh:name "sensor user" ;
				  sh:description "Kontakt, der die Daten aufgenommen hat."@de ;
				  sh:description "Contact of data acquisitor."@en ;
				  skos:example "company C" ;
				  sh:order 8 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:node vcard:Individual ] ;
	### documentation attributes ###
	sh:property [ sh:path device:CalibrationProcedure;
				  sh:name "calibration procedure" ;
				  sh:description "Beschreibt einen Kalibrationsprozess."@de ;
				  sh:description "Represents a calibration procedure."@en ;
				  skos:example "..." ;
				  sh:order 9 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path device:MaintenanceProcedure;
				  sh:name "maintenance procedure" ;
				  sh:description "Beschreibt einen Wartungsprozess."@de ;
				  sh:description "Represents a maintenance procedure."@en ;
				  skos:example "..." ;
				  sh:order 10 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path device:QualityAssuranceProcedure;
				  sh:name "quality assurance procedure" ;
				  sh:description "Beschreibt ein Qualitätssicherungsverfahren."@de ;
				  sh:description "Represents a quality assurace procedure."@en ;
				  skos:example "..." ;
				  sh:order 11 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path device:Process;
				  sh:name "processing history" ;
				  sh:description "Ein Prozess generiert einen Datenoutput durch die Anwendung von Methoden/Algorithmen mit bestimmten Parametern von definierten Inputdaten."@de ;
				  sh:description "A process that takes one or more inputs, and based on parameters and methodologies, generates one or more outputs. (SensorML.) As a System, a Process can be a composite of (sub)processes."@en ;
				  skos:example "..." ;
				  sh:order 12 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	### spatial attributes ###
	sh:property [ sh:path dcterms:temporal ;
				  sh:name "temporal coverage" ;
				  sh:description "Zeitliche Periode in der die Datenresource aufgenommen worden ist."@de ;
				  sh:description "Temporal coverage of when the data resource was acquired."@en ;
				  skos:example "'(a structure object of type CPU)'" ;
				  sh:order 13 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:node dcterms:PeriodOfTime ] ;
	sh:property [ sh:path dcterms:conformsTo ;
				  sh:name "spatial reference system" ;
				  sh:description "Räumliches Referenz System."@de ;
				  sh:description "Spatial reference system."@en ;
				  skos:example "'(a structure object of type CPU)'" ;
				  sh:order 17 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:node dcterms:Standard ] ;
	### temporal attributes ###
	sh:property [ sh:path dcterms:spatial ;
				  sh:name "spatial coverage" ;
				  sh:description "Räumliche Abdeckung wo die Datenresource aufgenommen worden ist."@de ;
				  sh:description "Spatial coverage of where the data resource was acquired."@en ;
				  skos:example "'(a structure object of type CPU)'" ;
				  sh:order 14 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:node dcterms:Location ] ;

### ??? NEEDS TO BE CORRECTED/DEFINED ON HOW THE SELF-DESCRIPTION IS LINKED TO THE DATASET IN THE DATASPACE ??? ###
	sh:property [ sh:path dcat:distribution ;
				  sh:name "distribution" ;
				  sh:description "A specific representation of a dataset. A dataset might be available in multiple serializations that may differ in various ways, including natural language, media-type or format, schematic organization, temporal and spatial resolution, level of detail or profiles (which might specify any or all of the above)."@de ;
				  sh:description "A specific representation of a dataset. A dataset might be available in multiple serializations that may differ in various ways, including natural language, media-type or format, schematic organization, temporal and spatial resolution, level of detail or profiles (which might specify any or all of the above)."@en ;
				  skos:example "'a link to the actual data'" ;
				  sh:order 15 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:node dcat:Distribution] ;
###########################################################################################################
.


dcterms:PeriodOfTime
	a sh:NodeShape ;
	sh:targetClass dcterms:PeriodOfTime ;
	sh:property [ sh:path dcat:startDate ;
				  sh:name "start date" ;
				  sh:description "Startdatum der Datenaufnahme (Datum Uhrzeit im ISO 8601-Format)."@de ;
				  sh:description "Start date of the data acquisition (date time in ISO 8601 format)."@en ;
				  skos:example "'2022-10-26T21:32:52'" ;
				  sh:order 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:dateTime ] ;
	sh:property [ sh:path dcat:endDate ;
				  sh:name "end date" ;
				  sh:description "Enddatum der Datenaufnahme (Datum Uhrzeit im ISO 8601-Format)."@de ;
				  sh:description "End date of the data acquisition (date time in ISO 8601 format)."@en ;
				  skos:example "'2022-10-26T21:32:52'" ;
				  sh:order 2 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:dateTime ] ;
.

dcterms:Standard
	a sh:NodeShape ;
	sh:targetClass  dcterms:Standard ;
	sh:property [ sh:path dcterms:identifier ;
				  sh:name "epsg code" ;
				  sh:description "Räumliches Referenz System definiert durch den EPSG Code."@de ;
				  sh:description "Spatial reference system defined as the EPSG code, e.g., 'EPSG:4326' (http://inspire.ec.europa.eu/glossary/SpatialReferenceSystem)"@en ;
				  skos:example "'EPSG:4326'" ;
				  sh:order 1 ;
				  sh:maxCount 1 ;
				  dcterms:type <http://inspire.ec.europa.eu/glossary/SpatialReferenceSystem>] ;
.

dcterms:Location
	a sh:NodeShape ;
	sh:targetClass dcterms:Location ;
	sh:property [ sh:path locn:geometry ;
				  sh:name "spatial coverage" ;
				  sh:description "Geometrische Information über die räumliche Abdeckung (z.B., as WKT, as KML, as GeoJSON)."@de ;
				  sh:description "Geometry information of spatial coverage (e.g., as WKT, as KML, as GeoJSON)."@en ;
				  skos:example "'e.g., as WKT, as KML, as GeoJSON - https://opengeospatial.github.io/ogc-geosparql/geosparql11/index.html#Geometry'" ;
				  sh:order 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
.

vcard:Individual
	a sh:NodeShape ;
	sh:targetClass vcard:Individual ;
	sh:property [ sh:path vcard:fn ;
				  sh:name "contact name" ;
				  sh:description "Name der Kontaktperson."@de ;
				  sh:description "Name associated to the contact person."@en ;
				  skos:example "'John Doe'" ;
				  sh:order 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path vcard:role ;
				  sh:name "contact role" ;
				  sh:description "Die Rolle, Aufgabe oder der Arbeitsbereich der Kontaktperson."@de ;
				  sh:description "The role, occupation, or business category of the contact person."@en ;
				  skos:example "'Surveyor'" ;
				  sh:order 2 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path vcard:org;
				  sh:name "contact organisation" ;
				  sh:description "Organisation bei der die Kontaktperson tätig ist."@de ;
				  sh:description "Affiliation of the contact person."@en ;
				  skos:example "'John Doe Company'" ;
				  sh:order 3 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path vcard:email;
				  sh:name "contact e-mail" ;
				  sh:description "E-Mail Adresse der Kontaktperson."@de ;
				  sh:description "Email address of the contact person."@en ;
				  skos:example "'johndoe@e-mail.com'" ;
				  sh:order 4 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
.

