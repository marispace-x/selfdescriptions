# QUESTIONS

# Why is there a '#' at the end of the other packages/namespaces URLs?


gax-validation:sensorSideScanSonarShape
	a sh:NodeShape ; 
	sh:targetClass ssn:System ;
	# Example: Frequency 100/400 kHz, 300/600 kHz
	sh:property [ sh:path ssn-system:SystemProperty ;
				  sh:name "acoustic center frequency" ;
				  sh:description "Akustische Frequenz die vom Sonar gesendet wird in [Hz]."@de ;
				  sh:description "Acoustic center frequency emitted by the sonar in [Hz]."@en ;
				  skos:example "300000" ;
				  sh:order 1 ;
				  sh:minCount 0 ;
				  sh:maxCount 2 ;
				  sh:datatype xsd:integer
				  ] ;

	# Pulse Length	100 kHz up to 20 ms 300 kHz up to 10 ms 400 kHz up to 10 ms 600 kHz up to 5 ms
	sh:property [ sh:path ssn-system:SystemProperty ;
				  sh:name "pulse length" ;
				  sh:description "Sonar Pulslänge die vom Sonar gesendet wird in [ms]."@de ;
				  sh:description "Sonar pulse length emitted by the sonar in [ms]."@en ;
				  skos:example "20" ;
				  sh:order 2 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:integer
				  ] ;

	# Example: Output Power	100 kHz: 4 joules, 400 kHz: 2 joules ; 300 kHz: 2 joules, 600 kHz: 1 joule
	# Question is wether the dependency to the frequency can be expressed
	sh:property [ sh:path ssn-system:OperatingPowerRange ;
				  sh:name "output power" ;
				  sh:description "Energie die vom Sonar abgegeben wird in [J]."@de ;
				  sh:description "Energy being emitted by sensor in [J]."@en ;
				  skos:example "4" ;
				  sh:order 3 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:integer
				  ] ;

	# Example: Spatial Resolution Across Track	100 kHz: 8 cm, 400 kHz: 2 cm ; 300 kHz: 3 cm, 600 kHz: 1.5 cm
	sh:property [ sh:path ssn-system:SystemProperty ;
				  sh:name "spatial resolution across track" ;
				  sh:description "Räumliche Auflösung in across track Richtung in [m]."@de ;
				  sh:description "Spatial resolution across track in [m]."@en ;
				  skos:example "3.0" ;
				  sh:order 4 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:decimal
				  ] ;

	# Example: Spatial Resolution Along Track	100 kHz: 2.5m @ 200 meter range; 400 kHz: 0.5m @ 100 meter range; 300 kHz: 1.0 m @ 200 meter range; 600 kHz: 0.45 m @ 100 meter range
	# ? can this be expressed as feture of interest -> property ?
	sh:property [ sh:path ssn-system:SystemProperty ;
				  sh:name "spatial resolution along track." ;
				  sh:description "Räumliche Auflösung in along track Richtung in [m]."@de ;
				  sh:description "Spatial resolution along track in [m]."@en ;
				  skos:example "3.0" ;
				  sh:order 5 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:decimal
				  ] ;

	# Example: Horizontal Beam Width	100 kHz: 0.64°, 400 kHz: 0.3° 300 kHz: 0.28°, 600 kHz: 0.26°
	sh:property [ sh:path ssn-system:SystemProperty ;
				  sh:name "beam opening angle along track" ;
				  sh:description "Strahlöffnungswinkel in along track Richtung (auch bezeichnet als Horizontale Strahlbreite) in [°]."@de ;
				  sh:description "Beam opening angle in along track direction (also called horizontal beam width) in [°]."@en ;
				  skos:example "0.64" ;
				  sh:order 6 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:decimal
				  ] ;

	# Example: Vertical Beam Width 50.0°
	sh:property [ sh:path ssn-system:SystemProperty ;
				  sh:name "beam opening angle along track" ;
				  sh:description "Strahlöffnungswinkel in across track Richtung (auch bezeichnet als Vertikale Strahlbreite) in [°]."@de ;
				  sh:description "Beam opening angle in across track direction (also called vertical beam width) in [°]."@en ;
				  skos:example "50.0" ;
				  sh:order 7 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:decimal
				  ] ;

	# Example: Heading Accuracy: < 1.5° RMS
	# https://www.w3.org/TR/vocab-ssn/#SSNSYSTEMAccuracy
	# again question is, wether accuracy can only be referred to the observed propoerty
	sh:property [ sh:path ssn-system:Accuracy;
				  sh:name "Heading Accuray" ;
				  sh:description "Kursgenauigkeit in [°] (RMS)"@de ;
				  sh:description "Heading accuracy in [°] (RMS)"@en ;
				  skos:example "1.5" ;
				  sh:order 8 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:decimal
				  ] ;

	# Example: Heading Resolution: 0.1°
	# https://www.w3.org/TR/vocab-ssn/#SSNSYSTEMResolution
	# same question as for accuracy
	sh:property [ sh:path ssn-system:Resolution;
				  sh:name "Heading Resolution" ;
				  sh:description "Kursauflösung in [°]."@de ;
				  sh:description "Heading resolution in [°]."@en ;
				  skos:example "0.1" ;
				  sh:order 9 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:decimal
				  ] ;

	# Example: Roll Angle Accuracy: ± 0.4°
	# https://www.w3.org/TR/vocab-ssn/#SSNSYSTEMAccuracy
	# again question is, wether accuracy can only be referred to the observed propoerty
	sh:property [ sh:path ssn-system:Accuracy ;
				  sh:name "Roll, Pitch Angle Accuracy" ;
				  sh:description "Rollen Winkelgenauigkeit in [°]."@de ;
				  sh:description "Roll angle accuracy in [°]."@en ;
				  skos:example "0.4" ;
				  sh:order 10 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:decimal
				  ] ;

	# Example: Roll Angle Resolution: 0.1°
	# https://www.w3.org/TR/vocab-ssn/#SSNSYSTEMResolution
	# same question as for accuracy
	sh:property [ sh:path ssn-system:Resolution ;
				  sh:name "roll and pitch resolution" ;
				  sh:description "Rollen Winkelauflösung in [°]."@de ;
				  sh:description "Roll angle resolution in [°]."@en ;
				  skos:example "0.1" ;
				  sh:order 11 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:decimal
				  ] ;

	# Example: Pitch Angle Accuracy: ± 0.4°
	# https://www.w3.org/TR/vocab-ssn/#SSNSYSTEMAccuracy
	# again question is, wether accuracy can only be referred to the observed propoerty
	sh:property [ sh:path ssn-system:Accuracy ;
				  sh:name "Roll, Pitch Angle Accuracy" ;
				  sh:description "Stampfen Winkelgenauigkeit in [°]."@de ;
				  sh:description "Pitch angle accuracy in [°]."@en ;
				  skos:example "0.4" ;
				  sh:order 12 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:decimal
				  ] ;

	# Example: Pitch Angle Resolution: 0.1°
	# https://www.w3.org/TR/vocab-ssn/#SSNSYSTEMResolution
	# same question as for accuracy
	sh:property [ sh:path ssn-system:Resolution ;
				  sh:name "roll and pitch resolution" ;
				  sh:description "Stampfen Winkelauflösung in [°]."@de ;
				  sh:description "Pitch angle resolution in [°]."@en ;
				  skos:example "0.1" ;
				  sh:order 13 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:decimal
				  ] ;


#### OPERATING PROPOERTIES ####

	# Example: Operating Temperature	0° C to 45°C
	sh:property [ sh:path ssn-system:OperatingRange ;
				  sh:name "operating temperature" ;
				  sh:description "Anwendungstemperatur in [°C]."@de ;
				  sh:description "Operating temperature in [°C]."@en ;
				  skos:example "0" ;
				  sh:order 14 ;
				  sh:minCount 0 ;
				  sh:maxCount 2 ;
				  sh:datatype xsd:integer
				  ] ;

	# Example: Operating Depth (meters)	2000 (SS) / 300 (Alum) tow bodies
	# https://www.w3.org/TR/vocab-ssn/#SSNSYSTEMOperatingRange
	# When speaking of a range in this case two values would potentially make sense (0 - 2000m)?
	# !!! find out how to ask for two values here
	sh:property [ sh:path ssn-system:OperatingRange ;
				  sh:name "operating depth" ;
				  sh:description "Anwendungstiefe in [m]."@de ;
				  sh:description "operating depth in [m]."@en ;
				  skos:example "2000" ;
				  sh:order 15 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:integer
				  ] ;
	#Towing Speed (max safe)	12 knots -> not yet included (could be added by value range? but this would then only be valid for this specific sensor not sidescansonars in general)
	#Towing Speed *	4.8 knots in HDM, 9.6 knots in HSM
	#https://www.w3.org/TR/vocab-ssn/#SSNSYSTEMOperatingProperty
	sh:property [ sh:path ssn-system:OperatingProperty ;
				  sh:name "towing speed" ;
				  sh:description "Maximale Geschwindigkeit mit der das Sonar geschleppt werden kann in [m/s]."@de ;
				  sh:description "Maximum towing speed of the sonar in [m/s]."@en ;
				  skos:example "300" ;
				  sh:order 16 ;
				  sh:minCount 0 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:integer
				  ] ;


