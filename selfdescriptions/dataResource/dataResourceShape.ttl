gax-validation:DataResourceShape
	a sh:NodeShape ;
	sh:targetClass gax-trust-framework:DataResource ;
	sh:property [ sh:path gax-trust-framework:producedBy ;
				  sh:name "produced by" ;
				  sh:description "Ein auflösbarer Link zur Selbstbeschreibung der Teilnehmer ermöglicht legal die Datennutzung."@de ;
				  sh:description "A resolvable link to the participant self-description legally enabling the data usage."@en ;
				  skos:example "'(a reference to gax-core:Participant object)'" ;
				  sh:order 1 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:nodeKind sh:IRI ] ;
	sh:property [ sh:path gax-trust-framework:exposedThrough ;
				  sh:name "exposed through" ;
				  sh:description "Eine auflösbare Link zu der Datenaustauschkomponente, die die Datenressource enthüllt."@de ;
				  sh:description "A resolvable link to the data exchange component that exposes the data resource."@en ;
				  skos:example "''" ;
				  sh:order 2 ;
				  sh:minCount 1 ;
				  sh:nodeKind sh:IRI ] ;
	sh:property [ sh:path gax-trust-framework:obsoleteDateTime ;
				  sh:name "obsolete date time" ;
				  sh:description "Datum Uhrzeit im ISO 8601 -Format, nach dem Daten veraltet sind."@de ;
				  sh:description "Date time in ISO 8601 format after which data is obsolete."@en ;
				  skos:example "'2022-10-26T21:32:52'" ;
				  sh:order 3 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:dateTime ] ;
	sh:property [ sh:path gax-trust-framework:expirationDateTime ;
				  sh:name "expiration date time" ;
				  sh:description "Datum Uhrzeit im ISO 8601 -Format, wonach die Daten abgelaufen sind und gelöscht werden."@de ;
				  sh:description "Date time in ISO 8601 format after which data is expired and shall be deleted."@en ;
				  skos:example "'2022-10-26T21:32:52'" ;
				  sh:order 4 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:dateTime ] ;
	sh:property [ sh:path gax-trust-framework:containsPII ;
				  sh:name "contains p i i" ;
				  sh:description "Boolesche, bestimmt vom Teilnehmer, der die Datenressource besitzt."@de ;
				  sh:description "Boolean determined by Participant owning the Data Resource."@en ;
				  skos:example "'True'" ;
				  sh:order 5 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:boolean ] ;
	sh:property [ sh:path gax-trust-framework:legalBasis ;
				  sh:name "legal basis" ;
				  sh:description "HINWEIS: Obligatorisch, wenn containSpii wahr ist.Einer der Gründe, wie in den identifizierten personenbezogenen Datenschutzregimen wie [GDPR2018] beschrieben.Potenzielle Rechtsgrundlagen können [Artikel 6], Artikel 7, (https://eur-lex.europa.eu/legal-content/en/txt/html/?uri=celex:32016r0679&from=en#d1e1888-1-1) oder Artikel 9. Es ist als Zeichenfolge, die 6.1. [A-F], 6.1.4, 7 oder 9.2.(Hinweis: Diese Liste ist nicht endgültig, da die DSGVO und das Mitglied des Mitgliedstaates zusätzliche rechtliche Grundlage vorsehen. Diese werden in zukünftigen Iterationen als Optionen implementiert.)"@de ;
				  sh:description "NOTE: Mandatory if containsPII is true. One of the reasons as detailed in the identified Personal Data Protection Regimes, such as [GDPR2018]. Potential Legal Bases can be [article 6], article 7, (https://eur-lex.europa.eu/legal-content/EN/TXT/HTML/?uri=CELEX:32016R0679&from=EN#d1e1888-1-1) or article 9. It shall be expressed as a string matching 6.1.[a-f], 6.1.4, 7 or 9.2.[a-j]. (Note: this list is not final, as GDPR and Member State Law may provide for additional legal basis. Those will be implemented as options in future iterations.)"@en ;
				  skos:example "'https://eur-lex.europa.eu/legal-content/EN/TXT/HTML/?uri=CELEX:32016R0679&from=EN#d1e1888-1-1'" ;
				  sh:order 6 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path gax-trust-framework:copyrightOwnedBy ;
				  sh:name "copyright owned by" ;
				  sh:description "Der als Gaia-X-Teilnehmer oder als andere Agent, möglicherweise auch als Person außerhalb von Gaia-X, angegebene Urheberrechtsbesitzer.Ein Urheberrechtsinhaber ist eine Person oder Organisation, die das Recht hat, die Ressource auszunutzen.Der Besitzer des Urheberrechts bezieht sich nicht auf den Autor der Ressource, der eine natürliche Person ist und sich vom Urheberrechtsinhaber unterscheidet.Eine einfache Namenszeichenfolge kann als leerer Knoten bezeichnet werden, dessen FOAF: Name -Attribut diesen Zeichenfolgewert hat."@de ;
				  sh:description "The copyright owner(s), given as a Gaia-X Participant or as some other agent, possibly also a person outside of Gaia-X. A copyright owner is a person or organization, that has the right to exploit the resource. Copyright owner does not necessary refer to the author of the resource, who is a natural person and may differ from copyright owner. A simple name string can be referenced as a blank node whose foaf:name attribute has that string value."@en ;
				  skos:example "'https://gaia-x.eu'" ;
				  sh:order 1 ;
				  sh:minCount 1 ;
				  sh:node gax-validation:AgentShape ] ;
	sh:property [ sh:path gax-trust-framework:license ;
				  sh:name "license" ;
				  sh:description "Eine Liste der SPDX -Lizenzkennungen oder URL zu Lizenzdokument"@de ;
				  sh:description "A list of SPDX license identifiers or URL to license document"@en ;
				  skos:example "'https://gaia-x.eu'" ;
				  sh:order 2 ;
				  sh:minCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path gax-trust-framework:policy ;
				  sh:name "policy" ;
				  sh:description "Eine Liste von Richtlinien, die mit einem DSL (z. B. Rego oder ODRL) ausgedrückt wurden (Zugangskontrolle, Drossel, Nutzung, Aufbewahrung,…)"@de ;
				  sh:description "A list of policy expressed using a DSL (e.g., Rego or ODRL) (access control, throttling, usage, retention, …)"@en ;
				  skos:example "'https://gaia-x.eu'" ;
				  sh:order 3 ;
				  sh:minCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path gax-trust-framework:name ;
				  sh:name "name" ;
				  sh:description "Name der Ressource."@de ;
				  sh:description "Name of resource."@en ;
				  skos:example "'Example Resource'" ;
				  sh:order 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path dct:description ;
				  sh:name "description" ;
				  sh:description "Eine detailliertere Beschreibung der Ressource."@de ;
				  sh:description "A more detailed description of resource."@en ;
				  skos:example "'Example Resource placed somewhere in Europe'" ;
				  sh:order 2 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path gax-trust-framework:aggregationOf ;
				  sh:name "aggregation of" ;
				  sh:description "Ressourcen im Zusammenhang mit der Ressource und das kann unabhängig davon existieren."@de ;
				  sh:description "Resources related to the resource and that can exist independently of it."@en ;
				  skos:example "'(a reference to gax-trust-framework:Resource object)'" ;
				  sh:order 3 ;
				  sh:nodeKind sh:IRI ] ;
	sh:property [ sh:path gax-core:aggregationOf ;
				  sh:name "aggregation of" ;
				  sh:description "Habe die Selbstbeschreibung der Ressourcen in Bezug auf die Ressource in Bezug auf die Ressourcen und das unabhängig davon existieren."@de ;
				  sh:description "DID of resources self-description related to the resource and that can exist independently of it."@en ;
				  skos:example "''" ;
				  sh:order 1 ;
				  sh:nodeKind sh:IRI ] ;
	sh:property [ sh:path gax-core:operatedBy ;
				  sh:name "operated by" ;
				  sh:description "Habe die Selbstbeschreibung der Teilnehmer im Zusammenhang mit dem Teilnehmer, der diese Ressource betreibt, in Bezug auf den Teilnehmer bezogen."@de ;
				  sh:description "DID of participant self-description related to the participant, who operates this resource."@en ;
				  skos:example "'https://gaia-x.eu'" ;
				  sh:order 2 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:nodeKind sh:IRI ] ;
.

gax-validation:AgentShape
	a sh:NodeShape ;
	sh:targetClass foaf:Agent ;
	sh:property [ sh:path foaf:name ;
				  sh:name "name" ;
				  sh:description "Ein Name für etwas.Verwenden Sie VCARD: Agent und seine umfassenderen Namensmöglichkeiten, wenn Sie mehr Ausdruckskraft benötigen."@de ;
				  sh:description "A name for some thing. Use vcard:Agent and its more comprehensive naming facilities when you need more expressiveness."@en ;
				  skos:example "'example-name'" ;
				  sh:order 1 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
.
