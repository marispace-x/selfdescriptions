# comments
gax-validation:DataResourceShape
	a sh:NodeShape ;
	sh:targetClass gax-trust-framework:dataResource ;
	sh:property [ sh:path dcterms:Sensorname ;
				  sh:name "sensor name" ;
				  sh:description "Der Name des Sensors."@de ;
				  sh:description "A name given to the sensor."@en ;
				  skos:example "'Dura Spark 400 (Applied Acoustics)'" ;
				  sh:order 1 ;
				  sh:minCount 1 ;
  				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
# questionable if that is a common attribute, for shure value range is not applicable but axamplary for providing dropdown choice of values
# potentially helpful for maybe
	sh:property [ sh:path dcterms:tipConfiguration ;
				  sh:name "tip configuration" ;
				  sh:description "Anzahl der installierten tips."@de ;
				  sh:description "number of tips installed."@en ;
				  sh:order 2 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:integer ] ;
# form creator does not recognize float values				  
	sh:property [ sh:path dcterms:pulseLength ;
				  sh:name "pulse length" ;
				  sh:description "Länge des Pulssignals; abhängig der Eingangsleistung."@de ;
				  sh:description "length of the pulse signal; depending on power."@en ;
				  skos:example "'1.5'" ;
				  sh:order 3 ;
				  sh:minCount 1 ;
  				  sh:maxCount 1 ;
				  sh:datatype xsd:double ;
				  sh:minInclusive 0 ;
				  sh:maxInclusive 3 ;
				] ;
	sh:property [ sh:path dcterms:soundOutput ;
				  sh:name "sound output" ;
				  sh:description "Schallstärke in db"@de ;
				  sh:description "sound output in db"@en ;
				  skos:example "223" ;
				  sh:order 4 ;
				  sh:minCount 1 ;
  				  sh:maxCount 1 ;
				  sh:datatype xsd:integer ;
				] ;
	sh:property [ sh:path dcterms:energy ;
				  sh:name "energy" ;
				  sh:description "Energie in J"@de ;
				  sh:description "engergy in J"@en ;
				  skos:example "1750" ;
				  sh:order 5 ;
				  sh:minCount 1 ;
  				  sh:maxCount 1 ;
				  sh:datatype xsd:integer ;
				] ;
	sh:property [ sh:path dcterms:pingIntervall ;
				  sh:name "ping intervall" ;
				  sh:description "Schussintervall in Hz"@de ;
				  sh:description "ping intervall in Hz"@en ;
				  skos:example "1" ;
				  sh:order 6 ;
				  sh:minCount 1 ;
  				  sh:maxCount 1 ;
				  sh:datatype xsd:integer ;
				] ;
	sh:property [ sh:path dcterms:sampleRate ;
				  sh:name "sample rate" ;
				  sh:description "Abtastrate in Hz"@de ;
				  sh:description "sample rate in Hz"@en ;
				  skos:example "1" ;
				  sh:order 7 ;
				  sh:minCount 1 ;
  				  sh:maxCount 1 ;
				  sh:datatype xsd:integer ;
				] ;
# CHANGE: currently string should take two values (integer or float)
	sh:property [ sh:path dcterms:antiAliasingFilter ;
				  sh:name "anti aliasing filter" ;
				  sh:description "Filter Bandbreite in Hz"@de ;
				  sh:description "filter bandwidth in Hz"@en ;
				  skos:example "140Hz - 10Hz" ;
				  sh:order 8 ;
				  sh:minCount 1 ;
  				  sh:maxCount 1 ;
				  sh:datatype xsd:string ;
				] ;
	sh:property [ sh:path dcterms:surveyVelocity ;
				  sh:name "survey velocity" ;
				  sh:description "Schiffsgeschwindingkeit bei Messung in kn"@de ;
				  sh:description "survey ship velocity in kn"@en ;
				  skos:example "4kn" ;
				  sh:order 9 ;
				  sh:minCount 1 ;
  				  sh:maxCount 1 ;
				  sh:datatype xsd:integer ;
				] ;


